sudo Firecoind -daemon -txindex
sudo mongo
cd explorer
sudo npm start

sudo /usr/bin/nodejs scripts/sync.js index update
sudo /usr/bin/nodejs scripts/sync.js index check
sudo /usr/bin/nodejs scripts/sync.js index reindex
sudo /usr/bin/nodejs scripts/sync.js market reindex

sudo /usr/bin/nodejs scripts/peers.js index update
sudo /usr/bin/nodejs scripts/peers.js index check
sudo /usr/bin/nodejs scripts/peers.js index reindex
sudo /usr/bin/nodejs scripts/peers.js market reindex
